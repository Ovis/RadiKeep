{
  "name": "RadiKeep",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run sync:vendors && tsc && npm run build:css && npm run build:scss",
    "build:css": "tailwindcss -i ./Styles/tailwind.css -o ./wwwroot/css/site.css --minify",
    "build:scss": "node ./Scripts/build-scss.mjs",
    "sync:vendors": "node ./Scripts/sync-vendor-assets.mjs"
  },
  "devDependencies": {
    "sass": "^1.97.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "hls.js": "^1.6.15",
    "ts-node": "^10.9.2"
  }
}
